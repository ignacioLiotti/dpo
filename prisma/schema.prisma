  @@index([IdEmpresa], map: "cert_IdEmpresa")
  @@index([IdObras], map: "IdObra")
  @@index([IdTipos], map: "IdTipos")
  @@index([NumCertificado], map: "NumCertificado")
  @@index([IdObras], map: "ObrasCertificaciones")
