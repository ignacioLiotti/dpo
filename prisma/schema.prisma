  @@index([item_id], map: "item_id")
}

model presupuestos {
  id          Int      @id @default(autoincrement())
  obraId      Int
  data        Json
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
  obra        obras    @relation(fields: [obraId], references: [IdObras])

  @@index([obraId])
}
